extends layout

block content
  .container
    #tabs
      ul.nav.nav-tabs(role="tablist")
        li
          a(data-toggle="tab", href="#objectList", role="tab") Liste des objets
        li
          a(data-toggle="tab", href="#myobjects", role="tab") Mes objets

      #objectList
        h4 Magasin
        table.table
          thead
            tr
              th Nom
              th Prix d'achat
              th Actions
          tbody
      #myobjects
        h4 Mes objets
        table.table
          thead
            tr
              th Nom
              th Prix de vente
              th(colspan="3") Actions
          tbody


  script(src='/javascripts/jquery-ui.js')
  script(src='/javascripts/objectscript.js')

